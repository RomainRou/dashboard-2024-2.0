              #03a9f499
            {% else %}
              #0000
            {% endif %}
          primary: '[[primary]]'
          secondary: null
          layout: horizontal
          tap_action:
            action: navigate
            navigation_path: '[[navigation]]'
          double_tap_action:
            action: none
